{
	"name": "alpaca-web-ui",
	"productName": "Alpaca Web UI",
	"description": "Alpaca Web UI built with Dalai, Express and Electron",
	"version": "1.0.0",
	"author": "Sergio Lara Guardiola",
	"license": "ISC",
	"main": "index.js",
	"icon": "icon/win/icon.ico",
	"scripts": {
		"test": "echo \"Error: no test specified\" && exit 1",
		"start": "electron .",
		"make-win": "npx electron-packager . --overwrite --platform=win32 --arch=x64 --icon=icon/win/icon.ico --prune=true --electron-version=13.6.9 --app-copyright=\"Laguse\" --out=release-builds  --version-string.CompanyName=Laguse",
		"pack-win": "node installers/windows/createinstaller.js",
		"win": "npm run make-win && npm run pack-win"
	},
	"dependencies": {
		"dalai": "^0.3.0",
		"express": "^4.18.2",
		"node-os-utils": "^1.3.7"
	},
	"devDependencies": {
		"electron": "^13.6.9",
		"electron-packager": "^17.1.1",
		"electron-winstaller": "^5.1.0"
	}
}
